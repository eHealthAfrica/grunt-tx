    "source-map-support": "^0.3.1"
  },
  "peerDependencies": {
    "grunt": "~0.4.5"
  },
  "keywords": [
    "grunt",
    "transifex",
    "translation",
    "i18n"
  ],
  "dependencies": {
    "babel": "^5.5.6",
