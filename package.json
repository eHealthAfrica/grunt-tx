    "test": "npm run lint && npm run build && mocha spec",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.14",
    "chai": "^3.0.0",
    "eslint": "^0.22.1",
